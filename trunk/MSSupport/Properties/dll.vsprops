	ProjectType="Visual C++"
	Version="8.00"
	Name="dll"
	>
	<Tool
		Name="VCCLCompilerTool"
		PreprocessorDefinitions="_DLL;_EXPORT_SYMBOLS"
	/>
	<Tool
		Name="VCLinkerTool"
		OutputFile="$(OutDir)\$(ProjectName)$(BuildSuffix).dll"
