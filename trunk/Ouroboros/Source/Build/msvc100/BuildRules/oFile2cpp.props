<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup
    Condition="'$(File2cppBeforeTargets)' == '' and '$(File2cppAfterTargets)' == '' and '$(ConfigurationType)' != 'Makefile'">
    <File2cppBeforeTargets>Midl</File2cppBeforeTargets>
    <File2cppAfterTargets>CustomBuild</File2cppAfterTargets>
  </PropertyGroup>
  <PropertyGroup>
    <File2cppDependsOn
      Condition="'$(ConfigurationType)' != 'Makefile'">_SelectedFiles;$(File2cppDependsOn);$(AdditionalDependencies)</File2cppDependsOn>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <File2cpp>
      <PrecompiledHeader></PrecompiledHeader>
      <BufferName>%(Filename)</BufferName>
      <Use64bitWords>false</Use64bitWords>
      <BigEndian>false</BigEndian>
      <File2cppOutput>$(IntDir)%(Filename).cpp</File2cppOutput>
      <CommandLineTemplate>"$(SolutionDir)bin\BuildTools\oFile2cpp.exe" [PrecompiledHeader] [BufferName] [Use64bitWords] [BigEndian] -i &quot;%(FullPath)&quot; [File2cppOutput]</CommandLineTemplate>
      <Outputs>%(File2cppOutput)</Outputs>
      <ExecutionDescription>oFile2cpp %(Filename)%(Extension)</ExecutionDescription>
    </File2cpp>
  </ItemDefinitionGroup>
</Project>