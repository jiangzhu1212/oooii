<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup
    Condition="'$(oRCVerBeforeTargets)' == '' and '$(oRCVerAfterTargets)' == '' and '$(ConfigurationType)' != 'Makefile'">
    <oRCVerBeforeTargets>Midl</oRCVerBeforeTargets>
    <oRCVerAfterTargets>CustomBuild</oRCVerAfterTargets>
  </PropertyGroup>
  <PropertyGroup>
    <oRCVerDependsOn
      Condition="'$(ConfigurationType)' != 'Makefile'">_SelectedFiles;$(oRCVerDependsOn);$(AdditionalDependencies)</oRCVerDependsOn>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <oRCVer>
      <Version>1.0</Version>
      <ProductName>$(ProjectName)</ProductName>
      <P4Root>//...</P4Root>
      <Includes></Includes>
      <AppIcon></AppIcon>
      <oRCVerOutput>$(IntDir)$(ProjectName).rc</oRCVerOutput>
      <CommandLineTemplate>"$(SolutionDir)bin\BuildTools\oRCVer.exe" [Version] [ProductName] [P4Root] [Includes] [AppIcon] -i &quot;%(FullPath)&quot; [oRCVerOutput]</CommandLineTemplate>
      <Outputs>%(oRCVerOutput)</Outputs>
      <ExecutionDescription>oRCVer $(ProjectName)%(Extension)</ExecutionDescription>
    </oRCVer>
  </ItemDefinitionGroup>
</Project>