<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup
    Condition="'$(FXCBeforeTargets)' == '' and '$(FXCAfterTargets)' == '' and '$(ConfigurationType)' != 'Makefile'">
    <FXCBeforeTargets>Midl</FXCBeforeTargets>
    <FXCAfterTargets>CustomBuild</FXCAfterTargets>
  </PropertyGroup>
  <PropertyGroup>
    <FXCDependsOn
      Condition="'$(ConfigurationType)' != 'Makefile'">_SelectedFiles;$(FXCDependsOn);$(AdditionalDependencies)</FXCDependsOn>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <FXC>
      <NoLogo>true</NoLogo>
      <EntryPoint>main</EntryPoint>
      <FXCOutput>$(IntDir)%(Filename)ByteCode.h</FXCOutput>
      <ByteCodeVariableName>%(Filename)ByteCode</ByteCodeVariableName>
      <ShaderModel>ps_5_0</ShaderModel>
      <DebugInformationFormat>0</DebugInformationFormat>
      <CommandLineTemplate>"$(DXSDK_DIR)\Utilities\bin\x86\fxc.exe" [NoLogo] [DebugInformationFormat] [EntryPoint] [ShaderModel] [Defines] [Includes] %(FullPath) /Fh [FXCOutput] [ByteCodeVariableName]</CommandLineTemplate>
      <Outputs>%(FXCOutput)</Outputs>
      <ExecutionDescription>FXC %(Filename)%(Extension)</ExecutionDescription>
    </FXC>
  </ItemDefinitionGroup>
</Project>