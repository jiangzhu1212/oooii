<?xml version="1.0" encoding="utf-8"?>
<oInputSetList>
  <oInputSet id="oMEDIA_INPUT" version="1.0">
    <oInput id="oMEDIA_INPUT_PLAY_PAUSE" Keys="oGUI_KEY_SPACE OR oGUI_KEY_P" />
    <oInput id="oMEDIA_INPUT_TRACK_NEXT_1" Keys="oGUI_KEY_1" />
    <oInput id="oMEDIA_INPUT_TRACK_NEXT_2" Keys="oGUI_KEY_2" />
    <oInput id="oMEDIA_INPUT_TRACK_PREV_1" Keys="oGUI_KEY_3" />
    <oInput id="oMEDIA_INPUT_TRACK_PREV_2" Keys="oGUI_KEY_4" />

    <oInputSequence
      id="oMEDIA_INPUT_TRACK_NEXT"
      MinTimeMS="100"
      MaxTimeMS="500"
      Seq="!oMEDIA_INPUT_TRACK_NEXT_1 oMEDIA_INPUT_TRACK_NEXT_2"
    />
    <oInputSequence
      id="oMEDIA_INPUT_TRACK_PREV"
      MinTimeMS="100"
      MaxTimeMS="500"
      Seq="!oMEDIA_INPUT_TRACK_PREV_1 oMEDIA_INPUT_TRACK_PREV_2"
    />
  </oInputSet>

  <oInputSet id="oSIMPLE_INPUT" version="1.0">
    <oInput id="oSIMPLE_INPUT_WAVE_LEFT_1" Keys="oGUI_KEY_A" />
    <oInput id="oSIMPLE_INPUT_WAVE_LEFT_2" Keys="oGUI_KEY_B" />
    <oInput id="oSIMPLE_INPUT_WAVE_RIGHT_1" Keys="oGUI_KEY_C" />
    <oInput id="oSIMPLE_INPUT_WAVE_RIGHT_2" Keys="oGUI_KEY_D" />

    <oInputSequence
      id="oSIMPLE_INPUT_WAVE"
      MinTimeMS="100"
      MaxTimeMS="1500"
      Seq="oSIMPLE_INPUT_WAVE_LEFT_1 !oSIMPLE_INPUT_WAVE_LEFT_1 oSIMPLE_INPUT_WAVE_LEFT_2 !oSIMPLE_INPUT_WAVE_LEFT_2 oSIMPLE_INPUT_WAVE_LEFT_1"
  />
    <oInputSequence
      id="oSIMPLE_INPUT_WAVE"
      MinTimeMS="100"
      MaxTimeMS="1500"
      Seq="oSIMPLE_INPUT_WAVE_RIGHT_1 !oSIMPLE_INPUT_WAVE_RIGHT_1 oSIMPLE_INPUT_WAVE_RIGHT_2 !oSIMPLE_INPUT_WAVE_RIGHT_2 oSIMPLE_INPUT_WAVE_RIGHT_1"
  />
  </oInputSet>
</oInputSetList>